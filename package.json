{
  "name": "ng-bcomponent-docs",
  "version": "0.0.1",
  "description": "An angular 2 template using gulp and bootstrap",
  "main": "index.html",
  "scripts": {
    "start": "concurrently \"npm run build:dev\" \"npm run server:dev\"",
    "start:dev": "npm start",
    "start:dist": "concurrently \"npm run build:dist\" \"npm run server:dist\"",
    "server:dev": "lite-server -c bs-config.js",
    "server:dist": "lite-server -c bs-config.dist.js",
    "build:dev": "gulp build:dev",
    "build:dist": "gulp build:dist",
    "compile:dev": "gulp index:dev",
    "compile:dist": "gulp index:dist",
    "postinstall": "typings install"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@angular/common": "^2.0.0-rc.6",
    "@angular/compiler": "^2.0.0-rc.6",
    "@angular/core": "^2.0.0-rc.6",
    "@angular/forms": "^2.0.0-rc.6",
    "@angular/http": "^2.0.0-rc.6",
    "@angular/platform-browser": "^2.0.0-rc.6",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.6",
    "@angular/router": "^3.0.0-beta.2",
    "@angular/upgrade": "^2.0.0-rc.5",
    "bootstrap": "^3.3.6",
    "core-js": "^2.4.0",
    "highlight.js": "^9.6.0",
    "jquery": "^2.2.4",
    "jquery-slimscroll": "^1.3.8",
    "malihu-custom-scrollbar-plugin": "^3.1.5",
    "ng-bcomponents": "^0.5.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.11",
    "systemjs": "0.19.36",
    "zone.js": "^0.6.17"
  },
  "devDependencies": {
    "concurrently": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-angular2-embed-templates": "^2.2.1",
    "gulp-concat": "^2.6.0",
    "gulp-inject": "^4.1.0",
    "gulp-typescript": "^2.13.6",
    "gulp-uglify": "^1.5.4",
    "gulp-uglifycss": "^1.0.6",
    "lite-server": "^2.2.2",
    "systemjs-builder": "^0.15.25",
    "typescript": "^1.8.10",
    "typings": "^1.0.4"
  }
}
